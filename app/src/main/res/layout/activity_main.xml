            android:textColor="@color/colorPrimary"
            android:textSize="@dimen/dp_20" />

        <EditText
            android:id="@+id/et_product_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="18"
            android:textColor="@color/colorPrimary"
            android:textSize="@dimen/dp_20"
            android:visibility="visible" />
    </LinearLayout>

    <Button
